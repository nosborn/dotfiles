# GitHub

Host github.com
  AddressFamily inet
  CheckHostIP yes
  Ciphers chacha20-poly1305@openssh.com
  HostKeyAlgorithms ssh-rsa
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519-github-nosborn-20180409
  KexAlgorithms curve25519-sha256@libssh.org
  RequestTTY no
  StrictHostKeyChecking yes
  User git

# GitLab

Host gitlab.com
  AddressFamily inet
  CheckHostIP yes
  Ciphers chacha20-poly1305@openssh.com
  HostKeyAlgorithms ssh-rsa
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
  KexAlgorithms curve25519-sha256@libssh.org
  RequestTTY no
  StrictHostKeyChecking yes
  User git

# Defaults.

Host *
  AddKeysToAgent yes
  Compression no
  ForwardAgent no
  ForwardX11 no
  PasswordAuthentication no
  PubkeyAuthentication yes
  ServerAliveInterval 20
  UseKeyChain yes
